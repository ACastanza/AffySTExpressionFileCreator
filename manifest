name=AffySTExpressionFileCreator
commandLine=<R3.1_Rscript> --no-save --quiet --slave --no-restore <libdir>run_gp_affyst_efc.R <libdir> <patches>/rlib/3.1_20141120/site-library \
    --input.file\=<input.file> --normalize=<normalize> --background.correct=<background.correct> --compute.present.absent.calls=<compute.present.absent.calls> \
    --qc.plot.format=<qc.plot.format> <clm.file> --annotate.probes=<annotate.probes> --output.file.base=<output.file.base>
license=
taskDoc=doc.html
fileFormat=gct;res;pdf;svg;png
taskType=Preprocess & Utilities
os=any
quality=preproduction
privacy=public
description=Creates a RES or GCT file from a set of CEL files from Affymetrix ST arrays.
version=Caption updates based on module documentation.
minGenePatternVersion=
language=R
author=David Eby, Broad Institute

p1_default_value=
p1_description=One or more Affymetrix ST CEL files.  These can be submitted directly through this parameter, packaged as ZIP or TAR bundles, or through a directory input.  The CEL files can be in GZ format and the TAR can be in GZ, XZ, or BZ2 format. One or more Affymetrix ST CEL files either uploaded directly, packaged into a ZIP or TAR bundle, or supplied through a directory input.  The CEL files can be in GZ format and the TAR can be in GZ, XZ, or BZ2 format.  The parameter will accept multiple inputs in any of these forms. 
p1_fileFormat=directory;cel;zip;gz;bz2;xz
p1_numValues=1+
p1_MODE=IN
p1_name=input.file
p1_optional=
p1_prefix_when_specified=
p1_TYPE=FILE
