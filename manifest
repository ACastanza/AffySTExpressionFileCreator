p2_description=Whether to normalize data using quantile normalization
p2_name=normalize
p2_optional=
p2_prefix_when_specified=
p2_type=java.lang.String
p2_value=yes;no

p3_default_value=yes
p3_description=Whether to perform background correction
p3_fileFormat=
p3_name=background.correct
p3_optional=
p3_prefix_when_specified=
p3_type=java.lang.String
p3_value=yes;no

p4_default_value=skip
p4_description=Choose a file format for the QC plots (or skip them completely)
p4_fileFormat=
p4_name=qc.plot.format
p4_optional=
p4_prefix_when_specified=
p4_type=java.lang.String
p4_value=pdf;svg;png;skip\=skip (do not produce QC plots)

p5_MODE=IN
p5_TYPE=FILE
p5_default_value=
p5_description=A tab-delimited text file containing one scan, sample, and class per line
p5_fileFormat=txt;clm
p5_name=clm.file
