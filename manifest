p3_description=Whether to perform background correction
p3_fileFormat=
p3_name=background.correct
p3_optional=
p3_prefix_when_specified=
p3_type=java.lang.String
p3_value=yes;no

p4_default_value=no
p4_description=Whether to compute Present/Absent calls.
p4_name=compute.present.absent.calls
p4_optional=
p4_prefix_when_specified=
p4_type=java.lang.String
p4_value=yes\=yes (create res file);no\=no (create gct file)

p5_default_value=skip
p5_description=Choose a file format for the QC plots (or skip them completely)
p5_fileFormat=
p5_name=qc.plot.format
p5_optional=
p5_prefix_when_specified=
p5_type=java.lang.String
